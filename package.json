{
  "name": "@nested-notion/tv",
  "description": "Add your favourite shows to Notion and get a schedule of upcoming episodes.",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "dev": "netlify dev",
