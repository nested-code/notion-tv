    "svelte-check": "^2.0.0",
    "svelte-loader": "^3.1.2",
    "svelte-preprocess": "^4.0.0",
    "ts-jest": "^27.0.5",
    "tslib": "^2.0.0",
    "typescript": "^4.4.2"
  },
  "dependencies": {
    "@netlify/functions": "^0.7.2",
    "carbon-components-svelte": "^0.42.2",
